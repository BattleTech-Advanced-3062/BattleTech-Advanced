      "idRef": {
        "id": "9c95d63381efb5c6d39cc3c1"
      },
      "index": 32,
      "text": "Alexander, no. I won't allow it.\n\nI can't afford to lose you.",
      "branches": [
        {
          "responseText": "",
          "conditions": null,
          "actions": null,
          "nextNodeIndex": 33,
          "hideIfUnavailable": false,
