          "actions": null,
          "nextNodeIndex": 2,
          "hideIfUnavailable": false,
          "onlyOnce": false,
          "idRef": {
            "id": "9ce912bcbbfa9ab263201b4e"
          },
          "inputBypass": false,
          "auxiliaryLink": false,
          "comment": ""
        },
        {
          "responseText": "If we're really that hard up for cash, we need to stop talking and start doing something about it.",
          "conditions": null,
          "actions": null,
          "nextNodeIndex": 23,
          "hideIfUnavailable": false,
          "onlyOnce": false,
          "idRef": {
