          },
          "inputBypass": false,
          "auxiliaryLink": false,
          "comment": ""
        },
        {
          "responseText": "We'll have to be. Your troops are needed looking after the noncombatants.",
          "conditions": null,
          "actions": null,
          "nextNodeIndex": 30,
          "hideIfUnavailable": true,
          "onlyOnce": false,
