    {
      "idRef": {
        "id": "9c21d40d5d676238eaab4450"
      },
      "index": 6,
      "text": "Damn right. Are you sure that the rest of the <i>Argo</i>'s systems are secure, Farah? I mean, <i>really</i> sure?",
      "branches": [
        {
          "responseText": "",
          "conditions": null,
          "actions": null,
          "nextNodeIndex": 7,
          "hideIfUnavailable": false,
          "onlyOnce": false,
