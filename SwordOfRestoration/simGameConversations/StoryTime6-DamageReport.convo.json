      "actions": null,
      "comment": ""
    },
    {
      "idRef": {
        "id": "9c33c0e1f6cf2912df1b52ed"
      },
      "index": 4,
      "text": "What we're going to do is leave the archive alone, powered down. That means no more [[DM.BaseDescriptionDefs[LoreStarLeague],Star League]] maps, no more hidden archive—nothing.\n\nPut some yellow tape around it if you need to—whatever you have to do to remind yourselves. Just… don't touch it.",
      "branches": [
        {
          "responseText": "",
          "conditions": null,
          "actions": null,
          "nextNodeIndex": 5,
          "hideIfUnavailable": false,
          "onlyOnce": false,
          "idRef": {
            "id": "9c437d0d0bf2ba2495f84571"
          },
          "inputBypass": false,
          "auxiliaryLink": false,
          "comment": ""
        }
      ],
      "nodeType": 1,
      "truthValue": false,
      "autoFollowBranchDelay": 1,
      "inputMaxLength": 0,
      "sourceTopicRef": null,
      "subjectTopicRefs": [],
      "sourceInSceneRef": null,
      "sourceWithTagInScene": "",
