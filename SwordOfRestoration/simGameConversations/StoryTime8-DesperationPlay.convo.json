    {
      "idRef": {
        "id": "9c97e104315e094de82bc5df"
      },
      "index": 10,
      "text": "I like how you think. I'll rig the explosives.",
      "branches": [
        {
          "responseText": "",
          "conditions": null,
          "actions": null,
          "nextNodeIndex": 11,
          "hideIfUnavailable": false,
          "onlyOnce": false,
